#dots {
  [zoom >= 9] {
    marker-fill: #ff0000;
    marker-line-color: #000000;
    marker-placement: point;
    marker-type: ellipse;
    marker-allow-overlap: true;
    [zoom = 9 ] { marker-line-width: 0.7 }
    [zoom = 10 ] { marker-line-width: 0.9 }
    [zoom = 11 ] { marker-line-width: 1.1 }
    [zoom = 12 ] { marker-line-width: 1.3 }
    [zoom = 13 ] { marker-line-width: 1.5 }
    [zoom = 14 ] { marker-line-width: 1.7 }
    [zoom >= 15 ] { marker-line-width: 1.9 }
    [zoom = 9 ] { marker-width: 3 }
    [zoom = 10 ] { marker-width: 4 }
    [zoom = 11 ] { marker-width: 5 }
    [zoom = 12 ] { marker-width: 6 }
    [zoom = 13 ] { marker-width: 7 }
    [zoom = 14 ] { marker-width: 8 }
    [zoom >= 15 ] { marker-width: 9 }
  }
}
